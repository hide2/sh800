<!-- content0 -->
<div id="container">
	<div id="content">
		<div style="font-size: 20px; margin: 10px 0 0 10px;">
			<a href="/message/<%= @m.id %>"><%= @m.title %></a>
		</div>
		<div style="border:1px solid #95D922;margin:10px;">
			<form style="padding:5px 10px" action="" method="POST" autocomplete="off">
				<input type="hidden" name="<%=request_forgery_protection_token%>" value="<%=form_authenticity_token%>" />
				<% if @password_wrong %>
				<div class="error">密码错误，请重新输入！</div>
				<% end %>
				<span class="title">请输入管理密码</span>
				<br/>
				<input type="password" name="password" class="input" value="" size="20" maxlength="20" autocomplete="off" />
				<span style="color:#666">管理密码就是信息发布时的密码。</span>
				<br/>
				<button type="submit" class="button" style="padding:2px 10px;font-size:14px;">确 定</button>
			</form>
		</div>
	</div>
	<div style="clear:both;"></div>
</div>
